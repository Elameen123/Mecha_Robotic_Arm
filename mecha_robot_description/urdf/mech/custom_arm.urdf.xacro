<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro">

  <xacro:macro name="custom_arm" params="prefix">

    <link name="${prefix}base_link">
      <visual>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="package://mecha_robot_description/meshes/mecha_robot/base_link.stl" scale="0.001 0.001 0.001"/>
        </geometry>
        <material name="silver"><color rgba="0.75 0.75 0.75 1.0"/></material>
      </visual>
      <collision>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="package://mecha_robot_description/meshes/mecha_robot/base_link.stl" scale="0.001 0.001 0.001"/>
        </geometry>
      </collision>
      <inertial>
         <mass value="0.145"/>
         <inertia ixx="0.000064" iyy="0.000115" izz="0.000064" ixy="0" iyz="0" ixz="0"/>
      </inertial>
    </link>

    <link name="${prefix}first_jaw_1">
      <visual>
        <origin xyz="0.018495 -0.02445 0.189098" rpy="0 0 0"/>
        <geometry>
          <mesh filename="package://mecha_robot_description/meshes/mecha_robot/first_jaw_1.stl" scale="0.001 0.001 0.001"/>
        </geometry>
        <material name="silver"/>
      </visual>
      <collision>
        <origin xyz="0.018495 -0.02445 0.189098" rpy="0 0 0"/>
        <geometry>
          <mesh filename="package://mecha_robot_description/meshes/mecha_robot/first_jaw_1.stl" scale="0.001 0.001 0.001"/>
        </geometry>
      </collision>
      <inertial>
        <mass value="0.465"/>
        <inertia ixx="0.00022" iyy="0.00027" izz="0.00037" ixy="0.00014" iyz="-0.00005" ixz="0.00006"/>
      </inertial>
    </link>

    <joint name="${prefix}joint1" type="continuous">
      <origin xyz="-0.018495 0.02445 -0.189098" rpy="0 0 0"/>
      <parent link="${prefix}base_link"/>
      <child link="${prefix}first_jaw_1"/>
      <axis xyz="0.0 -1.0 0.0"/>
    </joint>

    <link name="${prefix}second_jaw_1">
      <visual>
        <origin xyz="0.061323 -0.07045 0.181976" rpy="0 0 0"/>
        <geometry>
           <mesh filename="package://mecha_robot_description/meshes/mecha_robot/second_jaw_1.stl" scale="0.001 0.001 0.001"/>
        </geometry>
        <material name="silver"/>
      </visual>
      <collision>
        <origin xyz="0.061323 -0.07045 0.181976" rpy="0 0 0"/>
        <geometry>
           <mesh filename="package://mecha_robot_description/meshes/mecha_robot/second_jaw_1.stl" scale="0.001 0.001 0.001"/>
        </geometry>
      </collision>
      <inertial>
        <mass value="0.031"/>
        <inertia ixx="0.000034" iyy="0.000013" izz="0.000045" ixy="0" iyz="0" ixz="0"/>
      </inertial>
    </link>

    <joint name="${prefix}joint2" type="revolute">
      <origin xyz="-0.042828 0.046 0.007122" rpy="0 0 0"/>
      <parent link="${prefix}first_jaw_1"/>
      <child link="${prefix}second_jaw_1"/>
      <axis xyz="0.298 0.0 0.954"/>
      <limit upper="2.09" lower="-1.04" effort="100" velocity="100"/>
    </joint>

    <link name="${prefix}third_jaw_1">
      <visual>
        <origin xyz="0.013607 -0.157053 0.196915" rpy="0 0 0"/>
        <geometry>
           <mesh filename="package://mecha_robot_description/meshes/mecha_robot/third_jaw_1.stl" scale="0.001 0.001 0.001"/>
        </geometry>
        <material name="silver"/>
      </visual>
      <collision>
        <origin xyz="0.013607 -0.157053 0.196915" rpy="0 0 0"/>
        <geometry>
           <mesh filename="package://mecha_robot_description/meshes/mecha_robot/third_jaw_1.stl" scale="0.001 0.001 0.001"/>
        </geometry>
      </collision>
      <inertial>
        <mass value="0.0438"/>
        <inertia ixx="0.000007" iyy="0.000021" izz="0.00002" ixy="-0.000001" iyz="0" ixz="0.000005"/>
      </inertial>
    </link>

    <joint name="${prefix}joint3" type="continuous">
      <origin xyz="0.047716 0.086603 -0.014939" rpy="0 0 0"/>
      <parent link="${prefix}second_jaw_1"/>
      <child link="${prefix}third_jaw_1"/>
      <axis xyz="-0.298 0.0 -0.954"/>
    </joint>

    <link name="${prefix}fourth_jaw_1">
      <visual>
        <origin xyz="0.077637 -0.160859 0.162491" rpy="0 0 0"/>
        <geometry>
           <mesh filename="package://mecha_robot_description/meshes/mecha_robot/fourth_jaw_1.stl" scale="0.001 0.001 0.001"/>
        </geometry>
        <material name="silver"/>
      </visual>
      <collision>
        <origin xyz="0.077637 -0.160859 0.162491" rpy="0 0 0"/>
        <geometry>
           <mesh filename="package://mecha_robot_description/meshes/mecha_robot/fourth_jaw_1.stl" scale="0.001 0.001 0.001"/>
        </geometry>
      </collision>
      <inertial>
        <mass value="0.0174"/>
        <inertia ixx="0.000002" iyy="0.000003" izz="0.000003" ixy="0" iyz="0" ixz="0.000001"/>
      </inertial>
    </link>

    <joint name="${prefix}joint4" type="continuous">
      <origin xyz="-0.06403 0.003806 0.034424" rpy="0 0 0"/>
      <parent link="${prefix}third_jaw_1"/>
      <child link="${prefix}fourth_jaw_1"/>
      <axis xyz="0.946 0.127 -0.296"/>
    </joint>

    <link name="${prefix}fifth_jaw_1">
      <visual>
        <origin xyz="0.108766 -0.153426 0.154227" rpy="0 0 0"/>
        <geometry>
           <mesh filename="package://mecha_robot_description/meshes/mecha_robot/fifth_jaw_1.stl" scale="0.001 0.001 0.001"/>
        </geometry>
        <material name="silver"/>
      </visual>
      <collision>
        <origin xyz="0.108766 -0.153426 0.154227" rpy="0 0 0"/>
        <geometry>
           <mesh filename="package://mecha_robot_description/meshes/mecha_robot/fifth_jaw_1.stl" scale="0.001 0.001 0.001"/>
        </geometry>
      </collision>
      <inertial>
        <mass value="0.0614"/>
        <inertia ixx="0.000004" iyy="0.000005" izz="0.000005" ixy="0" iyz="0" ixz="0"/>
      </inertial>
    </link>

    <joint name="${prefix}joint5" type="revolute">
      <origin xyz="-0.031129 -0.007433 0.008264" rpy="0 0 0"/>
      <parent link="${prefix}fourth_jaw_1"/>
      <child link="${prefix}fifth_jaw_1"/>
      <axis xyz="-0.006 -0.910 -0.413"/>
      <limit upper="1.83" lower="-1.83" effort="100" velocity="100"/>
    </joint>

    <link name="${prefix}sixth_jaw_1">
       <visual>
          <origin xyz="0.13515 -0.152999 0.144544" rpy="0 0 0"/>
          <geometry>
             <mesh filename="package://mecha_robot_description/meshes/mecha_robot/sixth_jaw_1.stl" scale="0.001 0.001 0.001"/>
          </geometry>
          <material name="silver"/>
       </visual>
       <collision>
          <origin xyz="0.13515 -0.152999 0.144544" rpy="0 0 0"/>
          <geometry>
             <mesh filename="package://mecha_robot_description/meshes/mecha_robot/sixth_jaw_1.stl" scale="0.001 0.001 0.001"/>
          </geometry>
       </collision>
       <inertial>
         <origin xyz="-0.004 -0.0005 0.001" rpy="0 0 0"/>
         <mass value="0.0022"/>
         <inertia ixx="0.00001" iyy="0.00001" izz="0.00001" ixy="0" iyz="0" ixz="0"/>
       </inertial>
    </link>

    <joint name="${prefix}joint6" type="continuous">
        <origin xyz="-0.026384 -0.000427 0.009683" rpy="0 0 0"/>
        <parent link="${prefix}fifth_jaw_1"/> 
        <child link="${prefix}sixth_jaw_1"/>
        <axis xyz="0.946 0.127 -0.296"/>
    </joint>

  <gazebo reference="${prefix}first_jaw_1">
        <material>Gazebo/DarkGrey</material>
    </gazebo>

    <gazebo reference="${prefix}second_jaw_1">
        <material>Gazebo/DarkGrey</material>
    </gazebo>

    <gazebo reference="${prefix}third_jaw_1">
        <material>Gazebo/DarkGrey</material>
    </gazebo>

    <gazebo reference="${prefix}fourth_jaw_1">
        <material>Gazebo/Grey</material>
    </gazebo>

    <gazebo reference="${prefix}fifth_jaw_1">
        <material>Gazebo/Grey</material>
    </gazebo>

    <gazebo reference="${prefix}sixth_jaw_1">
        <material>Gazebo/Grey</material>
    </gazebo>

  </xacro:macro>
</robot>
